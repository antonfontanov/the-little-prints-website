{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,svg}",
      "/assets/*",
      "/favicon.ico",
      "/favicon.svg",
      "/*.{css,js,png,jpg,gif,svg,ico,json,woff,woff2,ttf,eot}"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/examples",
      "rewrite": "/index.html"
    },
    {
      "route": "/learn-more", 
      "rewrite": "/index.html"
    },
    {
      "route": "/order",
      "rewrite": "/index.html"
    },
    {
      "route": "/confirmation",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {}
}
